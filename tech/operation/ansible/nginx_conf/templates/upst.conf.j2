upstream {{ item.obj }}_{{ item.port }} {
        server {{ item.ip }}:{{ item.port }};
    }

server {
    listen  {{ item.w_port }};
    server_name {{ item.web_name }};

    error_log  logs/u_{{ item.obj }}_error.log;
    access_log  logs/u_{{ item.obj }}_access.log  main;

    #ssl on;
    # ssl_certificate /etc/letsencrypt/live/bs.com/fullchain.pem; #(证书公钥)
    # ssl_certificate_key /etc/letsencrypt/live/bs.com/privkey.pem; #(证书私钥)

    # ssl_session_timeout 5m;
    # ssl_protocols  SSLv2 SSLv3 TLSv1.2;
    # ssl_ciphers  HIGH:!aNULL:!MD5;
    # ssl_prefer_server_ciphers on;

    client_max_body_size 100m; 

    location / {
        proxy_cookie_path / "/; httponly; secure; SameSite=None";

        proxy_pass   http://{{ item.obj }}_{{ item.port }} ;
        index  index.html index.htm;
    }

    location ~ /ws/ {
    proxy_pass http://{{ item.obj }}_{{ item.port }};
    proxy_http_version 1.1;
    proxy_redirect off;
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_read_timeout 3600s;
    proxy_connect_timeout 40s;
    proxy_send_timeout 120s;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "Upgrade";
    }
}